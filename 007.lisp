#!/usr/local/bin/sbcl --script
(defun primep (num)
  (do ((div 2 (+ div 1)))
    ((> div (sqrt num)) t)
    (if (eql (mod num div) 0) (return nil))))
(defun gen-prime-list (n)
  (let ((lst nil))
    (do ((i 2 (+ 1 i)))
      ((>= (length lst) n) lst)
      (if (primep i) 
        (push i lst)))))
(defun euler ()
  (first (gen-prime-list 10001)))
(defun output-to-screen (x)
  (write-line x))
(output-to-screen (format nil "~a" (euler)))
